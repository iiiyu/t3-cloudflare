{
  "name": "t3-cloudflare",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "vercel-build": "next build",
    "db:up": "bun with-env drizzle-kit up",
    "db:generate": "bun with-env drizzle-kit generate",
    "db:push": "bun with-env drizzle-kit push --config drizzle.config.ts",
    "db:push:local": "bun with-env bun with-local-db drizzle-kit push --config drizzle.local.config.ts",
    "db:migrate": "bun with-env drizzle-kit migrate --config drizzle.config.ts",
    "db:migrate:local": "bun with-env bun with-local-db drizzle-kit migrate --config drizzle.local.config.ts",
    "db:check": "bun with-env drizzle-kit check --config drizzle.config.ts",
    "db:studio": "bun with-env drizzle-kit studio --config drizzle.config.ts",
    "db:studio:local": "bun with-env bun with-local-db drizzle-kit studio --config drizzle.local.config.ts",
    "d1:migrate:local": "wrangler d1 migrations apply t3-cloudflare --local",
    "d1:migrate:remote": "wrangler d1 migrations apply t3-cloudflare --remote",
    "dev": "bun with-env next dev",
    "lint": "bun with-env next lint",
    "pages:dev": "bun wrangler pages dev",
    "pages:build": "bun with-env next-on-pages",
    "preview": "bun pages:build && wrangler pages dev",
    "deploy": "bun pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "bindings": "cf-bindings-proxy",
    "with-env": "dotenv -e .dev.vars --",
    "with-local-db": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit)"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.2.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.40.1",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "cf-bindings-proxy": "^0.7.1",
    "drizzle-orm": "^0.31.2",
    "next": "14.1.0",
    "next-auth": "^5.0.0-beta.19",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.11.3",
    "@cloudflare/workers-types": "^4.20240605.0",
    "@types/better-sqlite3": "^7.6.10",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "better-sqlite3": "^9.6.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.22.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-next-on-pages": "^1.11.3",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vercel": "^34.2.6",
    "wrangler": "^3.60.1"
  },
  "ct3aMetadata": {
    "initVersion": "7.30.0"
  }
}
